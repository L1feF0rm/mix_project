{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.5.2"
    },
    "public/**": {
      "runtime": "@vercel/static"
    }
  },
  "routes": [{
    "src": "/(.*).(css|js|html)",
    "dest": "public/$1.$2", 
    "methods": ["GET"]
  }, {
    "src": "/(.*)",
    "dest": "/api/index.php"
  }]
}